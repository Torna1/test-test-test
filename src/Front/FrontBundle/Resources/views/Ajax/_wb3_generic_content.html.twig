<input type="button" name="name" value="Set language" class="set_language_button" data-zone-id="%zone_id%">
<table style="width:100%;" border="1">
    <tr>
        <td id="resizable_%zone_id%" class="code_frame" valign="top" style="width:50%">
            <textarea id="highlighter_textarea_%zone_id%" data-zone-id="%zone_id%" style="display: none;"></textarea>
        </td>
        <td class="result_frame" id="result_frame_%zone_id%" valign="top">
            <input type="button" id="execute_code_button_%zone_id%" data-zone-id="%zone_id%" value="Execute code">
            <iframe src="" id="code_execution_iframe_%zone_id%" style="width: 100%; height:90%; border:1px solid red;"></iframe>
        </td>
    </tr>
</table>