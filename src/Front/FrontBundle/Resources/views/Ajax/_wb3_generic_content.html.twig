    <option value="ocaml" data-mime="text/x-ocaml">OCaml</option>
    <option value="pascal" data-mime="text/x-pascal">Pascal</option>
    <option value="perl" data-mime="text/x-perl">Perl</option>
    <option value="php" data-mime="application/x-httpd-php">PHP</option>
    <option value="python" data-mime="text/x-python">Python</option>
    <option value="r" data-mime="text/x-rsrc">R</option>
    <option value="ruby" data-mime="text/x-ruby">Ruby</option>
